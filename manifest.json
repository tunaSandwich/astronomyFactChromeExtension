{
  "manifest_version": 2,
  "name": "Group Tab",
  "version": "0.1",
  "content_scripts": [
  {
    "matches": ["http://*/*", "https://*/*"],
    "js": ["content.js"]
  }
],
"browser_action": {
  "default_icon": "icon.png",
  "default_title": "Hello!"
},
"background": {
  "scripts": ["background.js"]
},
"permissions": [
          "notifications"
        ],
"web_accessible_resources": [
            "Andrameda.jpg"
]
      }
